{
  "name": "@variant/components",
  "version": "1.0.8",
  "private": false,
  "description": "Variant Styled Components",
  "author": "Mathias Fl√•tt <mf@variant.no>",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "echo \"Error: run tests from root\" && exit 1",
    "build": "tsc -p tsconfig.build.json && yarn prepare",
    "copy:css": "copyfiles -u 1 'src/**/*.{css,woff,woff2,ttf,eot,svg,png}' dist/",
    "copy:manifest": "copyfiles 'package.json' 'global.d.ts' 'README.md' dist/",
    "prepare": "yarn copy:css && yarn copy:manifest"
  },
  "devDependencies": {
    "copyfiles": "^2.3.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@variant/profile": "^1.0.6",
    "blobs": "^2.1.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-select": "^3.1.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
